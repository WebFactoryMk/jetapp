<%= if @friend %>
	<h1><%=@friend.profileName%></h1>
	<p> Do you really want to friend <%=@friend.profileName%>?</p>

	<%= form_for @user_friendship, :method: :post do |form| %>

		<div class="form form-aciton">
			<%= form.hidden_field :friend_id, value: @friend%>
			<%= submit_tag "Yes, Add Friend", class: "btn btn-flat"%>
			<%= link_to "Cancel", showProfile_path(@friend), class: "btn btn-flat"%>
		</div>
	<%end%>
<%end%>